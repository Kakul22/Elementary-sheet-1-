import java.util.*;
 class UserMainCode {
    public int SumlastDigits(int a,int b){
        int last1=Math.abs(a%10);
        int last2=Math.abs(b%10);
        return (last1 + last2);
    }
     public static void main(String[] args) {
         UserMainCode obj = new UserMainCode();
         System.out.println(obj.SumlastDigits(-25, 47)); // example run
     }
}
